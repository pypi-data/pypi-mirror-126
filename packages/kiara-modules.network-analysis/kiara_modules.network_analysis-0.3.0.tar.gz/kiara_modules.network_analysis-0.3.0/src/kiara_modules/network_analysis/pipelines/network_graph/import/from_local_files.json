{
  "module_type_name": "from_local_files",
  "documentation": "Create a network graph from a a csv file that contains edges. You can specify an additional csv file if you have node attributes.",
  "steps": [
    {
      "module_type": "import.file.from.file_path",
      "step_id": "read_edges_file"
    },
    {
      "module_type": "import.file.from.file_path",
      "step_id": "read_nodes_file"
    },
    {
      "module_type": "create.table.from.csv_file",
      "step_id": "load_edges_table",
      "input_links": {
        "value_item": "read_edges_file.file"
      }
    },
    {
      "module_type": "create.table.from.csv_file",
      "step_id": "load_nodes_table",
      "input_links": {
        "value_item": "read_nodes_file.file"
      }
    },
    {
      "module_type":  "network_graph.from_edges_table",
      "step_id": "create_graph",
      "input_links": {
        "edges_table": "load_edges_table.table"
      }
    },
    {
      "module_type": "network_graph.augment",
      "step_id": "augment_graph",
      "input_links": {
        "graph": "create_graph.graph",
        "node_attributes": "load_nodes_table.table"
      }
    }
  ],
  "input_aliases": {
    "read_edges_file__file_path": "edges_path",
    "read_nodes_file__file_path": "nodes_path",
    "create_graph__source_column": "source_column",
    "create_graph__target_column": "target_column",
    "create_graph__weight_column": "weight_column",
    "create_graph__graph_type": "graph_type",
    "augment_graph__nodes_attributes": "nodes_attributes",
    "augment_graph__index_column_name": "nodes_table_index",
    "save_graph__aliases": "aliases"
  },
  "output_aliases": {
    "augment_graph__graph": "graph"
  }
}

